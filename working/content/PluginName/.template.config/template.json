{
  "$schema": "http://json.schemastore.org/template",
  "identity": "CSSharpPluginTemplate",
  "name": "CSSharpPluginTemplate",
  "shortName": "css_template",
  "sourceName":"PluginName",
  "preferNameDirectory": true,
  "author": "hackmastr",
  "classifications": ["console"],
  "tags": {
    "language": "C#",
    "type":"project"
  },
  "symbols":{
    "ClassName":{
      "type": "parameter",
      "datatype": "text",
      "replaces": "PluginName",
      "fileRename": "PluginName",
      "defaultValue": "PluginName"
    },
    "ConfigName": {
      "type": "parameter",
      "description": "Plugin config name.",
      "datatype": "text",
      "replaces": "PluginNameConfig",
      "fileRename": "PluginNameConfig",
      "defaultValue": "PluginNameConfig"
    }
  }
}